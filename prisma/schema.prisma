generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Kisikisi {
  id Int @id @default(autoincrement())
  kategori String
  pertanyaan String
  jawaban String
  vektor Int[]
  createdAt DateTime @default(now())  
}
